    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "redirects":[
      {
        "source": "https://myfirstapp-env.eba-unha5y85.us-east-2.elasticbeanstalk.com/**",
        "destination": "http://myfirstapp-env.eba-unha5y85.us-east-2.elasticbeanstalk.com",
        "type": 302
      }

    ]
  }
}
